<h1>Bios</h2>

<% if user_signed_in? %>
  <%= link_to "New Bio", new_bio_path %>
<% end %>

<% @bios.each do |bio| %>
  <%= bio.name %> (<%= bio.instruments %>) 
  <% if user_signed_in? %>
    <%= link_to "Edit", edit_bio_path(bio) %>
  <% end %>

  <br>
  <p>
    <%= bio.summary %>
  </p>
<% end %>