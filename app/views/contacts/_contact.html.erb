<tr>
  <td><%= contact.name %></td>
  <td><%= contact.email %></td>
  <td><%= contact.message %></td>
  <td><%= contact.created_at.strftime("%m/%d/%Y") %></td>
  <td><%= contact.responded ? 'Yes' : 'No' %></td>
  <td>
    <%= button_to "Delete", {:controller => :contacts,
  :action => 'destroy', :id => contact.id }, :method => :delete, data: {confirm: "Are you sure?"} %>
  </td>
</tr>